# Paste your Google AI Studio API key here (if you want to use it)
GOOGLE_AI_STUDIO_API_KEY=

# If you run gpt-pilot as a local service, set its base URL (default expects http://localhost:8081)
GPT_PILOT_BASE_URL=http://localhost:8081

# Workspace storage directory
WORKSPACES_DIR=./workspaces

# Limits
MAX_PROJECT_SIZE_MB=20
CONCURRENCY=2
RETENTION_DAYS=7

# Simple anti-bot: minimum time (sec) user must spend on prompt page before generate
MIN_HUMAN_DELAY_SECONDS=2
